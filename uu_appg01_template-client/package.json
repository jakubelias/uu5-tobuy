{
  "name": "uu.apptemplate",
  "version": "0.1.0",
  "description": "Project template for developing UAF application with uu5.",
  "author": "Unicorn Solutions",
  "license": "MIT",
  "private": true,
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "npm run start-dev",
    "prestart-dev": "node tools/check-install.js && node tools/build-uc.js",
    "start-dev": "webpack-dev-server --hot --inline --compress --config tools/webpack.config.js",
    "prestart-prod": "npm run dist",
    "start-prod": "cross-env NODE_ENV=production webpack-dev-server --compress --config tools/webpack.config.js",
    "dist": "npm run dist-browser",
    "dist-browser": "cross-env NODE_ENV=production node tools/build-uc.js"
  },
  "engines": {
    "node": ">=6.9.2"
  },
  "dependencies": {
    "bootstrap": "3.3.7",
    "jquery": "3.2.1",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "systemjs": "0.19.47",
    "systemjs-plugin-json": "0.2.1",
    "uu5g04": "https://cdn.plus4u.net/uu-uu5g04/0.0.0/uu5g04.tgz",
    "uu_appg01": "https://cdn.plus4u.net/uu-appg01/1.0.0/uu_appg01.tgz",
    "uu_appg01_core": "https://cdn.plus4u.net/uu-appg01-core/1.0.0/uu_appg01_core.tgz",
    "uu_oidcg01": "https://cdn.plus4u.net/uu-oidcg01/1.0.0/uu_oidcg01.tgz"
  },
  "devDependencies": {
    "babel-core": "6.24.1",
    "babel-loader": "7.0.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "copy-webpack-plugin": "4.0.1",
    "cross-env": "5.0.0",
    "css-loader": "0.28.2",
    "ejs-loader": "0.3.0",
    "eslint": "3.19.0",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-react": "7.0.1",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.28.0",
    "http-server": "0.10.0",
    "less": "2.7.2",
    "less-loader": "4.0.3",
    "memory-fs": "0.4.1",
    "rimraf": "2.6.1",
    "style-loader": "0.18.0",
    "url-loader": "0.5.8",
    "webpack": "2.6.0",
    "webpack-dev-server": "2.4.5",
    "wrapper-webpack-plugin": "0.1.11",
    "write-file-webpack-plugin": "4.0.2"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "react"
    ]
  }
}
